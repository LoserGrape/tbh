local ABSection = ABTab:NewSection("< Ability Abuser >")

local ABSection = ABTab:NewSection("< WARNING: BEFORE USING ANYTHING PRESS THIS >")

ABSection:NewButton("Teleport To Arena", " ", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-15.3642788, -3.69053721, -7.41954088, 0.405183077, -0.00669269683, -0.914211094, -1.2553046e-05, 0.999973178, -0.00732610561, 0.914235532, 0.00297989813, 0.40517211)
		
end)

local ABSection = ABTab:NewSection("< Free Abilitys >")

ABSection:NewButton("Null Ability", " ", function()
    game:GetService("ReplicatedStorage"):WaitForChild("NullAbility"):FireServer()
end)

ABSection:NewButton("Rhythm Explosion", " ", function()
                local args = {
        [1] = "AoeExplosion",
        [2] = 42
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("rhythmevent"):FireServer(unpack(args))
end)

ABSection:NewButton("Blasphemy's Piano", "", function()
    game:GetService("ReplicatedStorage").Piano:FireServer()
end)

ABSection:NewToggle("SpeedRun WalkSpeed", " ", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 40
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 21
    end
end)

