local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("Repository Hub | Slap Battles", colors)

local colors = {
    SchemeColor = Color3.fromRGB(0,255,255),
    Background = Color3.fromRGB(0, 0, 0),
    Header = Color3.fromRGB(0, 0, 0),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}

local CDTab = Window:NewTab("Credit's")

local C1Section = CDTab:NewSection("My YouTube Channel | Creators Channel")

C1Section:NewButton("Copy My YouTube Channel Link", " ", function()
    setclipboard('https://www.youtube.com/@SlapBattlesRepository')
end)

local player = game.Players.LocalPlayer
local username = player.Name
local C2Section = CDTab:NewSection("And Thank You, " .. username .. ", For Using")
